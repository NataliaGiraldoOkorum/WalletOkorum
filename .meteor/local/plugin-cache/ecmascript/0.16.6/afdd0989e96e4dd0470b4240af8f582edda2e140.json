n":{"enter":[null]},"BinaryExpression":{"enter":[null]},"_exploded":true,"_verified":true},"options":{},"externalDependencies":[]}],"presets":[],"generatorOpts":{"filename":"/home/okorumdev/Documentos/meteor/hello-meteor/packages/mongo/remote_collection_driver.js","comments":true,"compact":false,"sourceMaps":true,"sourceFileName":"packages/mongo/remote_collection_driver.js"}},"code":"MongoInternals.RemoteCollectionDriver = function (mongo_url, options) {\n  var self = this;\n  self.mongo = new MongoConnection(mongo_url, options);\n};\nconst REMOTE_COLLECTION_METHODS = ['_createCappedCollection', '_dropIndex', '_ensureIndex', 'createIndex', 'countDocuments', 'dropCollection', 'estimatedDocumentCount', 'find', 'findOne', 'insert', 'rawCollection', 'remove', 'update', 'upsert'];\nObject.assign(MongoInternals.RemoteCollectionDriver.prototype, {\n  open: function (name) {\n    var self = this;\n    var ret = {};\n    REMOTE_COLLECTION_METHODS.forEach(function (m) {\n      ret[m] = _.bind(self.mongo[m], self.mongo, name);\n    });\n    return ret;\n  }\n});\n\n// Create the singleton RemoteCollectionDriver only on demand, so we\n// only require Mongo configuration if it's actually used (eg, not if\n// you're only trying to receive data from a remote DDP server.)\nMongoInternals.defaultRemoteCollectionDriver = _.once(function () {\n  var connectionOptions = {};\n  var mongoUrl = process.env.MONGO_URL;\n  if (process.env.MONGO_OPLOG_URL) {\n    connectionOptions.oplogUrl = process.env.MONGO_OPLOG_URL;\n  }\n  if (!mongoUrl) throw new Error(\"MONGO_URL must be set in environment\");\n  const driver = new MongoInternals.RemoteCollectionDriver(mongoUrl, connectionOptions);\n\n  // As many deployment tools, including Meteor Up, send requests to the app in\n  // order to confirm that the deployment finished successfully, it's required\n  // to know about a database connection problem before the app starts. Doing so\n  // in a `Meteor.startup` is fine, as the `WebApp` handles requests only after\n  // all are finished.\n  Meteor.startup(() => {\n    Promise.await(driver.mongo.client.connect());\n  });\n  return driver;\n});","map":{"version":3,"names":["MongoInternals","RemoteCollectionDriver","mongo_url","options","self","mongo","MongoConnection","REMOTE_COLLECTION_METHODS","Object","assign","prototype","open","name","ret","forEach","m","_","bind","defaultRemoteCollectionDriver","once","connectionOptions","mongoUrl","process","env","MONGO_URL","MONGO_OPLOG_URL","oplogUrl","Error","driver","Meteor","startup","Promise","await","client","connect"],"sources":["packages/mongo/remote_collection_driver.js"],"sourcesContent":["MongoInternals.RemoteCollectionDriver = function (\n  mongo_url, options) {\n  var self = this;\n  self.mongo = new MongoConnection(mongo_url, options);\n};\n\nconst REMOTE_COLLECTION_METHODS = [\n  '_createCappedCollection',\n  '_dropIndex',\n  '_ensureIndex',\n  'createIndex',\n  'countDocuments',\n  'dropCollection',\n  'estimatedDocumentCount',\n  'find',\n  'findOne',\n  'insert',\n  'rawCollection',\n  'remove',\n  'update',\n  'upsert',\n];\n\nObject.assign(MongoInternals.RemoteCollectionDriver.prototype, {\n  open: function (name) {\n    var self = this;\n    var ret = {};\n    REMOTE_COLLECTION_METHODS.forEach(\n      function (m) {\n        ret[m] = _.bind(self.mongo[m], self.mongo, name);\n      });\n    return ret;\n  }\n});\n\n// Create the singleton RemoteCollectionDriver only on demand, so we\n// only require Mongo configuration if it's actually used (eg, not if\n// you're only trying to receive data from a remote DDP server.)\nMongoInternals.defaultRemoteCollectionDriver = _.once(function () {\n  var connectionOptions = {};\n\n  var mongoUrl = process.env.MONGO_URL;\n\n  if (process.env.MONGO_OPLOG_URL) {\n    connectionOptions.oplogUrl = process.env.MONGO_OPLOG_URL;\n  }\n\n  if (! mongoUrl)\n    throw new Error(\"MONGO_URL must be set in environment\");\n\n  const driver = new MongoInternals.RemoteCollectionDriver(mongoUrl, connectionOptions);\n\n  // As many deployment tools, including Meteor Up, send requests to the app in\n  // order to confirm that the deployment finished successfully, it's required\n  // to know about a database connection problem before the app starts. Doing so\n  // in a `Meteor.startup` is fine, as the `WebApp` handles requests only after\n  // all are finished.\n  Meteor.startup(() => {\n    Promise.await(driver.mongo.client.connect());\n  });\n\n  return driver;\n});\n"],"mappings":"AAAAA,cAAc,CAACC,sBAAsB,GAAG,UACtCC,SAAS,EAAEC,OAAO,EAAE;EACpB,IAAIC,IAAI,GAAG,IAAI;EACfA,IAAI,CAACC,KAAK,GAAG,IAAIC,eAAe,CAACJ,SAAS,EAAEC,OAAO,CAAC;AACtD,CAAC;AAED,MAAMI,yBAAyB,GAAG,CAChC,yBAAyB,EACzB,YAAY,EACZ,cAAc,EACd,aAAa,EACb,gBAAgB,EAChB,gBAAgB,EAChB,wBAAwB,EACxB,MAAM,EACN,SAAS,EACT,QAAQ,EACR,eAAe,EACf,QAAQ,EACR,QAAQ,EACR,QAAQ,CACT;AAEDC,MAAM,CAACC,MAAM,CAACT,cAAc,CAACC,sBAAsB,CAACS,SAAS,EAAE;EAC7DC,IAAI,EAAE,UAAUC,IAAI,EAAE;IACpB,IAAIR,IAAI,GAAG,IAAI;IACf,IAAIS,GAAG,GAAG,CAAC,CAAC;IACZN,yBAAyB,CAACO,OAAO,CAC/B,UAAUC,CAAC,EAAE;MACXF,GAAG,CAACE,CAAC,CAAC,GAAGC,CAAC,CAACC,IAAI,CAACb,IAAI,CAACC,KAAK,CAACU,CAAC,CAAC,EAAEX,IAAI,CAACC,KAAK,EAAEO,IAAI,CAAC;IAClD,CAAC,CAAC;IACJ,OAAOC,GAAG;EACZ;AACF,CAAC,CAAC;;AAEF;AACA;AACA;AACAb,cAAc,CAACkB,6BAA6B,GAAGF,CAAC,CAACG,IAAI,CAAC,YAAY;EAChE,IAAIC,iBAAiB,GAAG,CAAC,CAAC;EAE1B,IAAIC,QAAQ,GAAGC,OAAO,CAACC,GAAG,CAACC,SAAS;EAEpC,IAAIF,OAAO,CAACC,GAAG,CAACE,eAAe,EAAE;IAC/BL,iBAAiB,CAACM,QAAQ,GAAGJ,OAAO,CAACC,GAAG,CAACE,eAAe;EAC1D;EAEA,IAAI,CAAEJ,QAAQ,EACZ,MAAM,IAAIM,KAAK,CAAC,sCAAsC,CAAC;EAEzD,MAAMC,MAAM,GAAG,IAAI5B,cAAc,CAACC,sBAAsB,CAACoB,QAAQ,EAAED,iBAAiB,CAAC;;EAErF;EACA;EACA;EACA;EACA;EACAS,MAAM,CAACC,OAAO,CAAC,MAAM;IACnBC,OAAO,CAACC,KAAK,CAACJ,MAAM,CAACvB,KAAK,CAAC4B,MAAM,CAACC,OAAO,EAAE,CAAC;EAC9C,CAAC,CAAC;EAEF,OAAON,MAAM;AACf,CAAC,CAAC"},"sourceType":"module","externalDependencies":{},"hash":"afdd0989e96e4dd0470b4240af8f582edda2e140"}
